<?xml version="1.0"?>
<fop version="1.0">
    <font-base>./src/main/resources/pdf/voucher/chemmart_free_voucher</font-base>
    <prefer-renderer>true</prefer-renderer>
    <renderers>
        <renderer mime="application/pdf">
            <fonts>

                <font kerning="yes" embed-url="OfficinaSansITCPro-Bold.ttf">
                    <font-triplet name="OfficinaSansITCPro-Bold" style="normal" weight="normal"/>
                </font>
                <font kerning="yes" embed-url="OfficinaSansITCPro-Book.ttf">
                    <font-triplet name="OfficinaSansITCPro-Book" style="normal" weight="normal"/>
                </font>
                <font kerning="yes" embed-url="OfficinaSanITCMedium.ttf">
                    <font-triplet name="OfficinaSanITCMedium" style="normal" weight="normal"/>
                </font>
                <font kerning="yes" embed-url="wingding.ttf">
                    <font-triplet name="Wingdings" style="normal" weight="normal"/>
                </font>
            </fonts>
        </renderer>
    </renderers>
</fop>

